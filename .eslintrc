{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true
  },
  "rules": {
    "no-unused-vars": "warn",
    "react/destructuring-assignment": ["warn" , "always"],
    "react/prop-types": "off",
    "indent": ["warn", 4]
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ]
}
